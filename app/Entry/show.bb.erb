<div id="pageTitle">
	<h1><%= @entry.title%></h1>
</div>

<div id="toolbar">
        <%= link_to "Back", :action => :index %>
        <%= link_to "Edit", :action => :edit, :id => @entry.object %>
</div>


<div id="content">

    <table>
      
      <tr>
    	<td class="itemLabel">Title</td>
        <td class="itemValue"><%= @entry.title%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Body</td>
        <td class="itemValue"><%= @entry.body%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">PhotoUri</td>
        <td class="itemValue"><%= @entry.photo_uri%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Datetime</td>
        <td class="itemValue"><%= @entry.datetime%></td>
      </tr>
      
    </table>
</div>